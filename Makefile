	$(NOECHO) $(EQUALIZE_TIMESTAMP) /System/Library/Perl/5.8.6/darwin-thread-multi-2level/CORE/perl.h $(INST_SCRIPT)/.exists

	-$(NOECHO) $(CHMOD) $(PERM_RWX) $(INST_SCRIPT)

